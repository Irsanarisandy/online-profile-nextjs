{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      ".app/*": ["app/*"],
      ".components/*": ["components/*"],
      ".data/*": ["data/*"],
      ".entities/*": ["entities/*"],
      ".images/*": ["public/images/*"],
      ".pages/*": ["pages/*"],
      ".styles/*": ["styles/*"],
      ".generatedTina/*": ["tina/__generated__/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [".next/types/**/*.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules", "cypress", "cypress.config.ts"]
}
